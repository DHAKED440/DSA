#include<iostream>
#include<vector>
using namespace std;

int findtarget(vector<int> &arr , int target){
    int n=arr.size();
     int st=0,end=n-1;
     while(st <=end ){
        int mid=st+(end-st)/2;
        if(arr[mid] == target){
            return mid;
        }
    //  if left side sorted hai 
     if(arr[mid] >= arr[st]){
        if(arr[st] <= target && target <= arr[mid]){
        end=mid-1;
        } else {
            st=mid+1;
        }
      } else {  // if right side sorted hai
        if( arr[mid]<= target && target<=arr[end]){
        st=mid+1;
     } else {
        end=mid-1;
     }
    }
 }
 return -1;
}


int main () {
    vector<int>arr={3,4,5,6,7,0,1,2};
    int target=0;

    int ans=findtarget(arr,target);
   if(target == arr[ans]){
    cout << "target is present" << endl;
   } else {
    cout << "target is not present" << endl;
   }
   return 0;
}

#include<iostream>
#include<limits.h>
using namespace std;

int main (){
   int n=7;
   int maxsum=INT_MIN;
   int arr[]={3, -4, 5, 4, -1, 7, -8};
   for(int st=0; st<n; st++){
      int cs=0;
      for(int end=st; end<n; end++){
       cs+=arr[end];
       maxsum=max(cs,maxsum);
      }
   } 
   cout<< "maxsubarray = " << maxsum << endl;
   return 0;
}
 


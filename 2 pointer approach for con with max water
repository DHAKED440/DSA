#include<iostream>
#include<vector>
using namespace std;

int conwithmaxwater(vector<int> &height){
    int n=height.size();
    int MW=0;
    int lp=0,rp=n-1;
   
    while(lp < rp){
            int w=rp-lp;
            int h=min(height[lp],height[rp]);
            int area=h*w;
            MW=max(MW,area);
            height[lp]<height[rp] ? lp++ : rp--;
    }
    return MW;
}
int main () {
    vector<int>height={1,8,6,2,5,4,8,3,7};
    int maxwater=conwithmaxwater(height);
    cout <<"containewr with  maxwater : " << maxwater << endl;
    return 0;
}

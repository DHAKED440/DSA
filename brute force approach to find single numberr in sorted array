#include<iostream>
#include<vector>
using namespace std;

int singlenumber( vector<int>& arr){
   for(int i=0 ; i<arr.size(); i++){
      if(arr[i] != arr[i+1] && arr[i] !=arr[i-1]){
        return arr[i];
      }
   }
}

int main () {
    vector<int>arr={1,1,2,3,3,4,4,8,8};
    cout << singlenumber(arr) << endl;
    return 0;
}
